{
  "scripts": {
    "test": "jest",
    "test:debug": "node --inspect node_modules/.bin/jest --runInBand",
    "test-watch": "jest --watch",
    "webpack:deploy": "webpack --config=webpack/webpack.config.js -p",
    "heroku-postbuild": "npm run webpack:deploy"
  },
  "jest": {
    "roots": [
      "app/javascript/__tests__"
    ],
    "moduleDirectories": [
      "node_modules",
      "app/javascript"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "./app/javascript/testSetup.js"
    ],
    "testRegex": "(./app/javascript/__tests__/.*|(\\.|/)(test|spec))\\.js?$"
  },
  "dependencies": {
    "@babel/core": "^7.0.1",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@rails/webpacker": "^4.0.2",
    "@types/jquery": "^3.3.29",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "bootstrap": "^4.3.1",
    "jest-css-modules": "^2.0.0",
    "jquery": "^3.4.1",
    "leaflet": "^1.4.0",
    "lodash": "^4.17.11",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-beautiful-dnd": "^10.0.3",
    "react-bootstrap": "^1.0.0-beta.8",
    "react-dom": "^16.8.6",
    "react-fade-in": "^0.1.6",
    "react-leaflet": "^2.2.1",
    "react-masonry-component": "^6.2.1",
    "react-router-dom": "^4.3.1",
    "reactstrap": "^8.0.0",
    "webpack": "^4.31.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-enzyme": "^1.0.0-beta.1",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "eslint": "5.3.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.0",
    "expect.js": "^0.3.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "jest-enzyme": "^7.0.1",
    "regenerator-runtime": "^0.13.2",
    "sinon": "^7.3.2",
    "undefined": "^0.1.0",
    "webpack-dev-server": "^3.4.1"
  }
}
